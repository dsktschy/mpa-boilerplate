  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsktschy/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/dsktschy/webpack-boilerplate#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "webpack": "^5.40.0",
